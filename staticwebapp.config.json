{
  "trailingSlash": "never",
  "routes": [
    {
      "route": "/index.html",
      "rewrite": "./public/index.html"
    },
    {
      "route": "./public/index.html",
      "rewrite": "./public/index.html"
    }
  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",

    "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
  }
}